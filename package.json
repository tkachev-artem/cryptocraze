{
  "name": "cryptocraze-fullstack",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/tsc -b && vite build --mode production",
    "build:server": "NODE_ENV=production ./node_modules/.bin/tsc -p tsconfig.server.json",
    "dev": "NODE_ENV=development vite --mode development",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "dev:cluster": "tsx server/cluster.ts",
    "start": "NODE_ENV=production node dist/server/index.cjs",
    "start:cluster": "NODE_ENV=production node dist/server/cluster.js",
    "start:dev": "NODE_ENV=development concurrently \"npm run redis:up && sleep 5 && npm run dev:server\" \"npm run dev\" --names \"server,client\" --prefix-colors \"blue,green\" --kill-others-on-fail",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "type-check": "./node_modules/.bin/tsc -b --noEmit",
    "fullstack": "npm run kill-ports && concurrently \"npm run dev:server\" \"npm run dev\" --names \"server,client\" --prefix-colors \"blue,green\" --kill-others-on-fail",
    "kill-ports": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true; lsof -ti:5173 | xargs kill -9 2>/dev/null || true; lsof -ti:5174 | xargs kill -9 2>/dev/null || true; lsof -ti:24678 | xargs kill -9 2>/dev/null || true; lsof -ti:4983 | xargs kill -9 2>/dev/null || true",
    "fullstack:full": "npm run kill-ports && concurrently \"npm run dev:server\" \"npm run dev\" \"npm run db:studio\" --names \"server,client,drizzle\" --prefix-colors \"blue,green,yellow\" --kill-others-on-fail",
    "fullstack:setup": "npm run redis:up && sleep 3 && npm run fullstack",
    "build:all": "concurrently \"npm run build\" \"npm run build:server\" --names \"client,server\" --prefix-colors \"green,blue\"",
    "redis:up": "docker-compose -f docker-compose.redis.yml up -d",
    "redis:down": "docker-compose -f docker-compose.redis.yml down",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:init": "tsx scripts/init-db.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@reduxjs/toolkit": "^2.6.1",
    "@socket.io/cluster-adapter": "^0.2.2",
    "@socket.io/redis-adapter": "^8.2.1",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.84.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/html2canvas": "^0.5.35",
    "@types/memoizee": "^0.4.12",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "bullmq": "^5.58.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "express": "^4.19.2",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "html2canvas": "^1.4.1",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "lightweight-charts": "^5.0.8",
    "lucide-react": "^0.511.0",
    "memoizee": "^0.4.17",
    "ogl": "^1.0.11",
    "openid-client": "^6.5.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "redis": "^4.6.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "three": "^0.167.1",
    "tsx": "^4.20.3",
    "ws": "^8.18.2",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/three": "^0.177.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/eslint-plugin": "^1.1.39",
    "concurrently": "^9.2.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.89.1",
    "terser": "^5.43.1",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.4",
    "vitest": "^3.1.1"
  }
}
